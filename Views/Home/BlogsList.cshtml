@model IEnumerable<authenticatedapp.Models.Blog>

<h2>Blog List</h2>


<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Title</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.name</td>
                <td>@user.title</td>
                <td>
                <a asp-controller="" asp-action="Delete" asp-route-id="@user.Id" class="btn btn-danger btn-sm">Delete</a>
                <a asp-controller="" asp-action="Update" asp-route-id="@user.Id" class="btn btn-primary btn-sm">Update</a>
                </td>
            </tr>
        }
    </tbody>
</table>
